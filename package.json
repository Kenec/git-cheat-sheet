{
  "name": "git-cheat-sheet",
  "version": "1.0.0",
  "description": "Git Cheat Sheet using MongoDB + Express + React + NodeJS",
  "main": "index.js",
  "scripts": {
    "start": "babel-node server/index.js",
    "test:client": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kenec/git-cheat-sheet.git"
  },
  "author": "Kenechukwu Nnamani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kenec/git-cheat-sheet/issues"
  },
  "homepage": "https://github.com/Kenec/git-cheat-sheet#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "css-loader": "^1.0.1",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "lodash": "^4.17.11",
    "path": "^0.12.7",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "^2.3.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.26.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.4",
    "coveralls": "^3.0.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "jest": "^23.6.0",
    "sinon": "^7.1.1",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/client/__tests__/setup/setupEnzyme.js",
    "testPathIgnorePatterns": [
      "<rootDir>/client/__tests__/setup/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/client/__mocks__/styleMock.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "client/components/**",
      "client/actions/*",
      "client/reducers/*"
    ]
  }
}
